<SyntaxDefinition name="Python" extensions=".py">
  <Properties>
    <Property name="LineComment" value="#" />
  </Properties>
  <Digits name="Digits" color="#d08356" />
  <RuleSets>
    <RuleSet ignorecase="false">
      <Delimiters>()[]{}@,:.`=;+-*/% &amp;|^&gt;&lt;</Delimiters>
      <Span name="Char" stopateol="true" color="#d08356" escapecharacter="\">
        <Begin>'</Begin>
        <End>'</End>
      </Span>
      <Span name="DocComment" color="#64686c">
        <Begin>"""</Begin>
        <End>"""</End>
      </Span>
      <Span name="LineComment" stopateol="true" color="#64686c">
        <Begin>#</Begin>
      </Span>
      <Span name="String" stopateol="true" color="#A5C160" escapecharacter="\">
        <Begin>"</Begin>
        <End>"</End>
      </Span>
      <MarkPrevious bold="true" color="#8a9597">(</MarkPrevious>
      <KeyWords name="BuiltInStatements" bold="true" color="#ceb67f">
        <Key word="assert" />
        <Key word="del" />
        <Key word="exec" />
        <Key word="global" />
        <Key word="lambda" />
        <Key word="print" />
      </KeyWords>
      <KeyWords name="ClassStatement" color="#DA4938" bold="true">
        <Key word="class" />
      </KeyWords>
      <KeyWords name="ExceptionHandlingStatements" bold="true" color="#ceb67f">
        <Key word="except" />
        <Key word="finally" />
        <Key word="raise" />
        <Key word="try" />
      </KeyWords>
      <KeyWords name="FunctionDefinition" bold="true" color="#a999ac">
        <Key word="def" />
      </KeyWords>
      <KeyWords name="Imports" bold="true" color="#CC7733">
        <Key word="import" />
        <Key word="from" />
      </KeyWords>
      <KeyWords name="IterationStatements" bold="true" color="#ceb67f">
        <Key word="for" />
        <Key word="in" />
        <Key word="while" />
      </KeyWords>
      <KeyWords name="JumpStatements" color="#ceb67f">
        <Key word="break" />
        <Key word="continue" />
        <Key word="yield" />
        <Key word="return" />
      </KeyWords>
      <KeyWords name="OperatorStatements" bold="true" color="#FF5FB8AD">
        <Key word="and" />
        <Key word="as" />
        <Key word="is" />
        <Key word="not" />
        <Key word="or" />
      </KeyWords>
      <KeyWords name="PassStatement" color="Gray">
        <Key word="pass" />
      </KeyWords>
      <KeyWords name="SelectionStatements" bold="true" color="#DA4938">
        <Key word="elif" />
        <Key word="else" />
        <Key word="if" />
      </KeyWords>
      <KeyWords name="WithStatement" color="#FF5FB8AD">
        <Key word="with" />
      </KeyWords>
    </RuleSet>
  </RuleSets>
</SyntaxDefinition>